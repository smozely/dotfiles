Host *
  UseKeychain yes
  AddKeysToAgent yes

# Old Atomic Config
Host gw gateway
  HostName gateway.atomic.io
  User gateway
  Port 777
  ForwardAgent yes
  ControlPath ~/.ssh/cm-%r@%h:%p
  ControlMaster auto
  ControlPersist 30m

Host 172.31.*
  User          ubuntu
  ProxyCommand  ssh gateway@gw nc %h %p 2> /dev/null
  IdentityFile ~/.ssh/gw_id_rsa
  StrictHostKeyChecking no
